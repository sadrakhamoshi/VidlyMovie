@model Vidly.Moveis.ListOfMovei
@{
    ViewBag.Title = "Movie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h1>Movie</h1>

    <div>
        <table class="table table-hover table-bordered">
            @foreach (var s in Model.MyMovie)
            {
                <thead>
                    <tr>
                        <th scope="auto">@s.Name</th>
                    </tr>
                </thead>
            }

        </table>
    </div>



</div>